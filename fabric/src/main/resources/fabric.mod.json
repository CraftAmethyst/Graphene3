{
    "schemaVersion": 1,
    "id": "tritium",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "sources": "https://github.com/CraftAmethyst/Tritium/"
    },
    "license": "${license}",
    "icon": "assets/${mod_id}/logo.png",
    "environment": "*",
  "entrypoints": {
        "main": [
          "org.craftamethyst.tritium.tritium"
        ],
    "modmenu": [
      "org.craftamethyst.tritium.integration.ModMenuIntegration"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
      "fabric-api": "*",
      "minecraft": ">=1.20 <1.20.2",
      "cloth-config": ">=11",
        "java": ">=17"
    },
    "suggests": {}
}
  
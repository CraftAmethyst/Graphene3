{
    "schemaVersion": 1,
    "id": "tritium",
    "version": "${version}",
  
    "name": "Tritium",
    "description": "An all-in-one optimization module dedicated to optimizing client-side rendering and server-side stability and fluency.",
    "authors": [
      "QianMo_ProMax",
      "ZCRAFT",
      "KSmc_brigade"
    ],
    "contact": {
      "sources": "https://github.com/CraftAmethyst/Tritium/"
    },
    "license": "MIT",
  "icon": "assets/tritium/logo.png",
  
    "environment": "*",
    "entrypoints": {
      "main": [
        "org.craftamethyst.tritium.tritium"
      ],
      "client": [
        "org.craftamethyst.tritium.client.FabricClientInitializer"
      ],
      "modmenu": [
        "org.craftamethyst.tritium.config.TritiumConfigScreenFactory"
      ]
    },
    "mixins": [
      "tritium.mixins.json",
      "tritium.fabric.mixins.json"
    ],
  
    "depends": {
      "fabricloader": ">=0.14",
      "fabric-api": "*",
      "minecraft": "1.19.x",
      "cloth-config": ">=8",
      "java": ">=17"
    },
  "suggests": {}
  }
  